<h1>New Movie</h1>

<%= form_tag movies_path, method: :post do %>
  <p><%= label_tag :title %>
  <%= text_field_tag :title %></p>

  <p><%= label_tag :year %>
    <%= text_field_tag :year %></p>


<p>  <%= label_tag :director %>
    <%= text_field_tag :director %>
  <!-- <%= select_tag :director_id, options_from_collection_for_select(Director.all, :id, :name) %> -->
</p>

  <%= submit_tag 'Add Movie' %>
<% end %>
